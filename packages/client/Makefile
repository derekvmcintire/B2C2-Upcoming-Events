run:
	npm run dev

proxy:
	vercel dev --yes

build:
	npm run build

lint:
	npm run lint

preview:
	npm run preview

typecheck:
	npm run typecheck

format:
	npm run format

add:
	npm run format; git add .

clean:
	npm run format; npm run typecheck; npm run lint

cyp:
	npx cypress run --spec "cypress/e2e/home/"